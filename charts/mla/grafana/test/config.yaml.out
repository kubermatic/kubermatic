# Source: grafana/charts/grafana/templates/configmap.yaml
apiVersion: v1
kind: ConfigMap
metadata:
  name: release-name-grafana
  namespace: default
  labels:
    helm.sh/chart: grafana-7.0.21
    app.kubernetes.io/name: grafana
    app.kubernetes.io/instance: release-name
    app.kubernetes.io/version: "10.2.2"
    app.kubernetes.io/managed-by: Helm
data:
  
  grafana.ini: |
    [analytics]
    check_for_updates = true
    [auth.proxy]
    auto_sign_up = true
    enabled = true
    header_name = X-Forwarded-Email
    header_property = username
    [grafana_net]
    url = https://grafana.net
    [log]
    mode = console
    [paths]
    data = /var/lib/grafana/
    logs = /var/log/grafana
    plugins = /var/lib/grafana/plugins
    provisioning = /etc/grafana/provisioning
    [server]
    domain = ''
  dashboardproviders.yaml: |
    apiVersion: 1
    providers:
    - disableDeletion: false
      folder: ""
      name: default
      options:
        path: /var/lib/grafana/dashboards/default
      org_id: 1
      type: file
    - disableDeletion: false
      folder: Kubernetes
      name: kubernetes
      options:
        path: /var/lib/grafana/dashboards/kubernetes
      org_id: 1
      type: file
    - disableDeletion: false
      folder: Kubermatic
      name: kubermatic
      options:
        path: /var/lib/grafana/dashboards/kubermatic
      org_id: 1
      type: file
  download_dashboards.sh: |
    #!/usr/bin/env sh
    set -euf
    mkdir -p /var/lib/grafana/dashboards/default
    mkdir -p /var/lib/grafana/dashboards/kubernetes
    mkdir -p /var/lib/grafana/dashboards/kubermatic
